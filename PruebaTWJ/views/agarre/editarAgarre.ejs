<div style='background-color:white;color:black;'>

  <h1>Edita tu agarre  </h1>

  <div class="row">
    <div class="col-sm-8">
      <form action="agarre/editarAgarre" method="post">

        <div class="form-group">
          <label>Nombre</label>
          <input required type="text" class="form-control"  placeholder="Nombre" name="nombre">
        </div>
        <div class="form-group">
          <label>Veces</label>
          <input required type="text" class="form-control"  placeholder="Veces" name="veces">
        </div>
        <div class="form-group">
          <label>Dinero Gastado</label>
          <input required type="text" class="form-control"  placeholder="Veces" name="dineroGastado">
        </div>

        <input type="hidden" name="id" value="<%=Agarre.id%>">
        <button type="submit" class="btn btn-success">Editar Agarre</button>
        <br>

      </form>
    </div>

    <div class="col-sm-4">
    </div>
  </div>


</div>
