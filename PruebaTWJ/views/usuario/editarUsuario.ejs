<div style='background-color:white;color:black;'>

  <h1>Editar Usuario  </h1>

  <div class="row">
    <div class="col-sm-8">
      <form action="usuario/editarUsuario" method="post">
        <div class="form-group">
          <label>Nombre</label>
          <input required type="text" class="form-control"  placeholder="Nombres" name="nombre">
        </div>

        <div class="form-group">
          <label>Preferencia</label>
          <input type="text" class="form-control" placeholder="Preferencia" name="preferencia">

        </div>
        <div class="form-group">
          <label>Fecha de Nacimiento</label>
          <input type="date" format="dd/MM/yyyy" class="form-control" placeholder="Fecha de Nacimiento" name="fechaNacimiento">
        </div>

        <input type="hidden" name="id" value="<%=Usuario.id%>">
        <button type="submit" class="btn btn-success">Editar Usuario</button>
        <br>

      </form>
    </div>
    <div class="col-sm-4">

    </div>
  </div>
</div>
